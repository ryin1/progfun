package forcomp
import forcomp.Anagrams._

object anagram_test {
  println("Welcome to the Scala worksheet")       //> Welcome to the Scala worksheet
  val occurrences = List(('a', 2), ('b', 5))      //> occurrences  : List[(Char, Int)] = List((a,2), (b,5))
  val keys = occurrences map (_._1)               //> keys  : List[Char] = List(a, b)
 	sentenceAnagrams(List("married", "player"))
  wordOccurrences("hello")                        //> res0: List[forcomp.Anagrams.Sentence] = List()
  dictionaryByOccurrences(wordOccurrences("eat")) //> res1: List[forcomp.Anagrams.Word] = List(ate, eat, tea)
}