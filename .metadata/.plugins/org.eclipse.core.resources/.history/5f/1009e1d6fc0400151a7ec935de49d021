package forcomp
import forcomp.Anagrams._

object anagram_test {
	val word = "asdfas"
	word groupBy (x => x) 
                                                  //| as), List(man, yes), List(men, say), List(as, en, my), List(as, my, en), Lis
                                                  //| t(sane, my), List(Sean, my), List(my, en, as), List(my, as, en), List(my, sa
                                                  //| ne), List(my, Sean), List(say, men), List(yes, man))
  }