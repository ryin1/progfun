package forcomp
import forcomp.Anagrams._

object anagram_test {
	val word = "asdfas"                       //> word  : String = asdfas
	(word groupBy (x => x)) map {case (k, v) => (k -> v.length)}
                                                  //> res0: scala.collection.immutable.Map[Char,Int] = Map(s -> 2, d -> 1, a -> 2,
                                                  //|  f -> 1)

  }